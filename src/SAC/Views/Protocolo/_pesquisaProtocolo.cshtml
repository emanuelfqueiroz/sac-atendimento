@model SAC.Controllers.Requests.PesquisaProtocolo


<div class="row">
    <div class="col-md-4">
        <form asp-action="index" method="get">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProtocoloId" class="control-label"></label>
                <input asp-for="ProtocoloId" class="form-control" />
                <span asp-validation-for="ProtocoloId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DigitroId" class="control-label"></label>
                <input asp-for="DigitroId" class="form-control" />
                <span asp-validation-for="DigitroId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Documento" class="control-label"></label>
                <input asp-for="Documento" class="form-control" />
                <span asp-validation-for="Documento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nome" class="control-label"></label>
                <input asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Index" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

